<form>
	<label name="company"/>
	<field name="company" readonly="1"/>
	<separator string="Current Capital" colspan="4"/>
	<label name="current_asset"/>
	<field name="current_asset" readonly="1"/>
	<label name="current_asset_amount"/>
	<field name="current_asset_amount" readonly="1"/>
	<label name="current_liability"/>
	<field name="current_liability" readonly="1"/>
	<label name="current_liability_amount"/>
	<field name="current_liability_amount" readonly="1"/>
	<newline/>
	<label name="current_capital" colspan="3"/>
	<field name="current_capital" readonly="1"/>
	<separator string="Recommended Capital" colspan="4"/>
	<label name="annual_budget"/>
	<field name="annual_budget" readonly="1"/>
	<newline/>
	<label name="recommended_capital"/>
	<field name="recommended_capital" readonly="1"/>
	<newline/>
	<separator string="Operative Capital" colspan="4"/>
	<label name="current_capital"/>
	<field name="current_capital" readonly="1"/>
	<newline/>
	<label name="recommended_capital"/>
	<field name="recommended_capital" readonly="1"/>
	<newline/>
	<label name="difference"/>
	<field name="difference" readonly="1"/>
	<newline/>
	<label name="percentage"/>
	<field name="percentage" readonly="1" xalign="1.0" xexpand="0"/>



</form>
