        <!-- PARTY -->

        <record model="ir.action.act_window" id="act_subscriber">
            <field name="name">Subscribers</field>
            <field name="res_model">party.party</field>
            <field name="domain" eval="[ 'AND', 
                    [('is_subscriber', '=', True),], 
                    [('company', '=', Eval('context', {}).get('company', -1))],
             ]" pyson="1"/>
          </record>

          <menuitem parent="menu_training" sequence="1"
            action="act_subscriber" id="menuitem_subscriber"
            icon="subscriptor_icon"/>

          <record model="ir.action.act_window" id="act_student">
            <field name="name">Students</field>
            <field name="res_model">party.party</field>
            <!--<field name="domain" eval="['AND', [('estudiante', '=', True)],
                                        [('company', '=', Eval('user', {}).get('company', None))],
                                        ]" pyson="1"/>-->
            <field name="domain" eval="[ 'AND', 
                    [('is_student', '=', True),], 
                    [('company', '=', Eval('context', {}).get('company', -1))],
             ]" pyson="1"/>
          </record>

        <record model="ir.action.act_window.view" id="act_student_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="student_view_tree"/>
            <field name="act_window" ref="act_student"/>
        </record>

        <record model="ir.action.act_window.view" id="act_student_form_view2">
